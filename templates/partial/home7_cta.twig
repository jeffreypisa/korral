<section id="hetontstaanvankorral" class="mod-cta" data-scroll-section>
	<div class="container-fluid">
		<div class="row g-6">
			<div class="col-lg-6 mb-5 mb-lg-0 anime-fadein">
				{% if post.cta_titel %}
					<h2 class="text-green h3">{{ post.cta_titel }}</h2>
				{% endif %}
				{% if post.cta_tekst %}
					{{ post.get_field('cta_tekst') }}
				{% endif %}
				{% if post.cta_link %}
					<br>
					<a class="btn btn-primary" href="{{ post.cta_link.url }}" target="{{ post.cta_link.target }}">{{ post.cta_link.title }}</a>
				{% endif %}
			</div>
			<div class="col-lg-6 anime-fadein">
				<div class="js-slick-cta">
					{% for item in post.get_field('cta_slider') %}
						<div class="item">
							<img src="{{ Image(item).src }})" alt="w-100 h-auto mb-4">		
						</div>
					{% endfor %}
				</div>
			</div>
		</div>
		<br>
		<div class="fish mt-5 ">
			<div class="row g-3">
				<div class="col">
					<img src="{{ theme.link }}/assets/img/fish_green.svg" height="30" alt="fish" class="w-100 h-auto">
				</div>
				<div class="col d-none d-md-block">
					<img src="{{ theme.link }}/assets/img/fish_green.svg" height="30" alt="fish" class="w-100 h-auto">
				</div>
				<div class="col d-none d-lg-block">
					<img src="{{ theme.link }}/assets/img/fish_green.svg" height="30" alt="fish" class="w-100 h-auto">
				</div>
				<div class="col d-none d-xl-block">
					<img src="{{ theme.link }}/assets/img/fish_green.svg" height="30" alt="fish" class="w-100 h-auto">
				</div>
			</div>
		</div>
	</div>
</section>